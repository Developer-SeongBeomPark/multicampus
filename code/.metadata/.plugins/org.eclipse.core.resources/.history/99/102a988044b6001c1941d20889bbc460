package day04.code;

import java.util.Scanner;

class operator{
	private int x, y;
	private String operator;
	Scanner sc = new Scanner(System.in);
	
	public int getX() {
		return x;
	}

	public void setX(int x) {
		this.x = x;
	}

	public int getY() {
		return y;
	}

	public void setY(int y) {
		this.y = y;
	}

	public String getOperator() {
		return operator;
	}

	public void setOperator(String operator) {
		this.operator = operator;
	}

	public void input() {
		Scanner sc = new Scanner(System.in);
		this.x = sc.nextInt();
		this.y = sc.nextInt();
		this.operator = sc.nextLine();
		
	}
	
	public int Oper(int x, int y, String operator) {
		int value;
		if(operator == "+") {
			value = x + y;
		}
		else if(operator == "-") {
			value = x - y;
		}
		else if(operator == "*") {
			value = x * y;
		}
		else {
			value = x / y;
		}
		return value;
	}
	
	public void print() {
		System.out.println("결과값 : " + Oper(this.x, this.y, this.operator));
	}
}

public class Operator {

	public static void main(String[] args) {
		operator op = new operator();
		
		op.input();
		op.print();

	}

}
