package day05;

class A{
	public A() {
		System.out.println("생성장 A");
	}
	
	public A(int x) {
		
	}
}

class B extends A{
	public B() {
		System.out.println("생성자 B");
	}
}

public class ConstructorEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		B b;
		b = new B();
		
	}

}
