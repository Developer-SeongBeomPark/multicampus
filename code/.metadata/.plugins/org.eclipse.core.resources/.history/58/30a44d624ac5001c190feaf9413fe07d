package day05;

class A{
	public A() {
		System.out.println("생성장 A");
	}
	
	public A(int x) {
		
	}
}

class B extends A{
	public B() {
		System.out.println("생성자 B");
	}
}

public class ConstructorEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		B b = new B();
		
	}

}

// 이 경우 Class A에서 기본생성자를 만들지 안을 경우 에러가 발생한다.
// 부모 클래스에 기본 생성자를 꼭 작성해주도록 한다.
