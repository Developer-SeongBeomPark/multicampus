package day09;

abstract class Sum{
  public abstract String toString();
}

class Sum_s1 extends Sum{

  @Override
  public String toString() {
    // TODO Auto-generated method stub
    return "class Sum_s1 extends Sum";
  }
}

class Sum_s2 extends Sum{

  @Override
  public String toString() {
    // TODO Auto-generated method stub
    return "class Sum_s2 extends Sum";
  }
  
}

public class SumMain {

  public static void main(String[] args) {
    // TODO Auto-generated method stub
    Sum_s1 s1 = new Sum_s1();
    String value = s1.toString();
    System.out.println("s1 tosting : " + value);
    
    Sum_s2 s2 = new Sum_s2();
    String value_2 = s2.toString();
    System.out.println("s2 tosting : " + value_2);
  }

}
